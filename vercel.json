{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "PLAYWRIGHT_BROWSERS_PATH": "0"
  },
  "buildCommand": "pip install -r requirements.txt && python -m playwright install chromium"
}
